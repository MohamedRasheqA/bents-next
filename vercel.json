{
  "version": 2,
  "builds": [
    {
      "src": "app/api/python/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/python/(.*)",
      "dest": "app/api/python/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
